{
  "name": "dalykraken-functions",
  "version": "2.0.0",
  "description": "DalyKraken 2.0 Firebase Functions",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
